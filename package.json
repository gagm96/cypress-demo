{
  "name": "cypress-course",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "regression-testing": "npx cypress run --browser chrome; node ./cypress/support/multiple-cucumber-html-reporter.js",
    "smoke-testing": "npx cypress run --env TAGS=\"@smoke\" --browser chrome; node ./cypress/support/multiple-cucumber-html-reporter.js",
    "sanity-testing": "npx cypress run --env TAGS=\"@sanity\" --browser chrome; node ./cypress/support/multiple-cucumber-html-reporter.js",
    "defect-testing": "npx cypress run --env TAGS=\"@defect\" --browser chrome; node ./cypress/support/multiple-cucumber-html-reporter.js"
  },
  "dependencies": {
    "@cucumber/cucumber": "^10.9.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "allure-commandline": "^2.30.0",
    "allure-cypress": "^3.0.0-beta.10",
    "cypress": "^13.13.3",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^3.7.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/e2e/bdd/tests",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
